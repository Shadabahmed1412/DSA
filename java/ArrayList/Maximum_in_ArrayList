
import java.util.ArrayList;



public class Maximum_in_ArrayList {
    public static void main(String[] args) {
        // DECARALATION OF AN ARRAY LIST 
        ArrayList <Integer> list = new ArrayList<>();
// ADDING AN ELEMENTS  INTO A LIST 

    list.add(2);
    list.add(1);
    list.add(5);
    list.add(6);
    list.add(8);
    int max=Integer.MIN_VALUE;
// printing an max element in a array list 
        for(int i =0 ; i<list.size();i++){
      
            if ( max < list.get(i))
            max=list.get(i);
        }
        System.out.println( " MAXIMUM ELEMENT IN AN AARRY LIST IS "+ max);
    }
    
}
